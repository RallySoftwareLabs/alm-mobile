<div>
    <table class="table table-striped">
        <thead>
            <th>Formatted ID</th>
            <th>Name</th>
            <th>State</th>
        </thead>
        <tbody>
        	{{#each defects}}
            <tr>
            	<td><a href="defect/{{this.ObjectID}}">{{this.FormattedID}}</a></td>
                <td>{{this.Name}}</td>
            	<td>{{this.ScheduleState}}</td>
            </tr>
        	{{/each}}
        </tbody>
    </table>
</div>
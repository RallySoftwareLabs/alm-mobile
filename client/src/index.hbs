<!doctype html>
<!--[if lt IE 7]> <html class="no-js lt-ie9 lt-ie8 lt-ie7" lang="en"> <![endif]-->
<!--[if IE 7]>    <html class="no-js lt-ie9 lt-ie8" lang="en"> <![endif]-->
<!--[if IE 8]>    <html class="no-js lt-ie9" lang="en"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js" lang="en"> <!--<![endif]-->
<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <title>Rally ALM Mobile</title>
  <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=no">
  <meta name="apple-mobile-web-app-capable" content="yes" />
  <meta name="apple-mobile-web-app-status-bar-style" content="black" />
  <link rel="stylesheet" href="/css/app.css">
  <link rel="stylesheet" href="/css/pageslider-2013-06-06.css">

  <link rel="apple-touch-icon-precomposed" href="/img/apple-touch-icon-57x57-precomposed.png" />
  <link rel="apple-touch-icon-precomposed" sizes="57x57" href="/img/apple-touch-icon-57x57-precomposed.png" />
  <link rel="apple-touch-icon-precomposed" sizes="72x72" href="/img/apple-touch-icon-72x72-precomposed.png" />
  
  <script type="text/javascript" src="//cdnjs.cloudflare.com/ajax/libs/require.js/2.1.10/require.min.js"></script>
  <script type="text/javascript">
    require.config({
      paths: {
        "jquery": "//cdnjs.cloudflare.com/ajax/libs/jquery/2.0.3/jquery.min",
        "jqueryBase64": "/js/jquery.base64.min",
        "bootstrap": "//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min",
        "backbone": "//cdnjs.cloudflare.com/ajax/libs/backbone.js/1.1.0/backbone",
        "spin": "//cdnjs.cloudflare.com/ajax/libs/spin.js/1.3.3/spin.min",
        "moment": "//cdnjs.cloudflare.com/ajax/libs/moment.js/2.5.1/moment.min",
        "pagedown": "//cdnjs.cloudflare.com/ajax/libs/pagedown/1.0/Markdown.Converter.min",
        "react": "//cdnjs.cloudflare.com/ajax/libs/react/0.9.0/react.min"
      },
      shim: {
        jquery: {
          exports: "$"
        },
        backbone: {
          deps: ["underscore", "jquery"],
          exports: "Backbone"
        },
        bootstrap: {
          deps: ["jquery"]
        },
        jqueryBase64: {
          deps: ["jquery"]
        },
        spin: {
          deps: ["jquery"]
        },
        moment: {
          exports: "moment"
        },
        md: {
          exports: "md"
        },
        pagedown: {
          exports: "Markdown"
        }
      },
      waitSeconds: 15
    });

    define('appConfig', function() {
      return {
        {{#each config}}
          "{{@key}}": "{{this}}",
        {{/each}}
      };
    });

    window.initializeApp = true;
  </script>
  <script src="/js/app.js"></script>
</head>
<body>
</body>
</html>

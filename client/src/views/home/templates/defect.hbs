<button class="btn btn-block" id="defect/{{defect.ObjectID}}">
    <div class="span11 item">
        <div class={{state defect.Blocked defect.Ready}}></div>
        <div class="item-id-name">
            <div class="formatted-id">{{defect.FormattedID}}</div>
            <div class="workitem-name">{{defect.Name}}</div>
        </div>
    </div>
    <div class="span1 chevron"><i class="icon-chevron-right"></i></div>
</button>

<div id="home-view">
  {{#if iteration}}
  <div class="iteration-header">
    <dl class="dl-horizontal">
      <dt>Iteration:</dt>
      <dd class="name">{{iteration.Name}}</dd>
      <dd class="dates">{{date iteration.StartDate}} - {{date iteration.EndDate}}</dd>
    </dl>
  </div>
  {{/if}}
  <div id="home-view-navigation">
    <ul class="nav btn-group">
      {{#each tabs}}
      <li class="btn{{#if this.active}} active{{/if}}" id="{{this.key}}-tab">
        <a href="/{{this.key}}" id="{{this.key}}">
          {{this.value}}
        </a>
      </li>
      {{/each}}
    </ul>
  </div>

  <div class="row-fluid listing"/>

  <div class='bottom-centered'>
    <a href="{{createRoute}}" class='btn active' id='add-artifact'><i class="icon-plus"/></a>
  </div>
</div>
<div id="home-view">
  <div id="home-view-navigation">
    <ul class="nav btn-group">
      {{#each tabs}}
      <li class="btn{{#if this.active}} active{{/if}}" id="{{this.key}}-tab">
        <a href="{{this.key}}" id="{{this.key}}" data-toggle="pill" data-target="#{{this.key}}-view">
          {{this.value}}
        </a>
      </li>
      {{/each}}
    </ul>
  </div>

  <div class="tab-content">
    {{#each tabs}}
    <div class="tab-pane{{#if this.active}} active{{/if}}" id="{{this.key}}-view">
      <div class="row-fluid listing"/>
    </div>
    {{/each}}
  </div>

  <div class='bottom-centered'>
    <a href="{{createRoute}}" class='btn active' id='add-artifact'>+</a>
  </div>
</div>